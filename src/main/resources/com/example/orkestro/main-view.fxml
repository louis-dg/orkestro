<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.ListView?>
<HBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.orkestro.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <ListView fx:id="groupListView"/>

    <ListView fx:id="tracksListView"/>

    <VBox prefWidth="400">
        <Label>TMP tracksPane</Label>
        <AnchorPane fx:id="tracksPane"/>

        <HBox spacing="10">
            <Button text="Lecture" onAction="#onPlayClick"/>
            <Button text="Stop" onAction="#onStopClick"/>
        </HBox>
    </VBox>
</HBox>
